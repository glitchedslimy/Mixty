{
  "name": "Slimed Commands",
  "version": "0.1.0",
  "root": "slimecommands",
  "description": "A command handler made custom for Mixty",
  "sourceRoot": "slimmedcommands/src",
  "projectType": "lib",
  "generators": {},
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc -p ./slimecommands/tsconfig.json"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "eslint --fix"
      }
    },
    "format": {
      "executor": "nx:run-commands",
      "options": {
        "command": "prettier --write \"slimecommands/src/**/*.ts\""
      }
    }
  }
}
